{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "ignoreDeprecations": "5.0",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "typeRoots": ["./src/types"],
    "types": ["config", "global", "module", "store", "vue-router"]
  },
}
